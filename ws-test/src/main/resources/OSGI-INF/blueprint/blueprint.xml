<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cxf="http://camel.apache.org/schema/blueprint/cxf"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <cxf:cxfEndpoint id="wsCrearTdC"
                     address="/eorder/crear-tdc"
                     wsdlURL="wsdl/SITDC501.wsdl">
        <cxf:properties>
            <entry key="dataFormat" value="PAYLOAD" />
        </cxf:properties>
    </cxf:cxfEndpoint>

    <camelContext xmlns="http://camel.apache.org/schema/blueprint"
                  id="edesur.eorder.ws-test"
                  useMDCLogging="true" >

        <route id="ws-test">
            <from uri="cxf:bean:wsCrearTdC"/>
            <log message="XXXX" loggingLevel="WARN" />
            <log message="${body.toString()}" loggingLevel="WARN" />
        </route>

    </camelContext>

</blueprint>